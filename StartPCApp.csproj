<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>false</UseWPF>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <AssemblyTitle>StartPCApp</AssemblyTitle>
    <AssemblyDescription>Windows系统管理工具，支持全局热键快速关机、定时关机和界面缩放调整</AssemblyDescription>
    <AssemblyCompany>StartPCApp Team</AssemblyCompany>
    <AssemblyProduct>StartPCApp</AssemblyProduct>
    <AssemblyCopyright>Copyright © 2024 StartPCApp Team</AssemblyCopyright>
    <AssemblyVersion>1.4.1.0</AssemblyVersion>
    <FileVersion>1.4.1.0</FileVersion>
    <Version>1.4.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2210.55" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="6.0.1" />
  </ItemGroup>

</Project>